{
  "name": "@fhevm/sdk",
  "version": "1.0.0",
  "description": "Core SDK for FHEVM - encryption, decryption, and key management utilities",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "keywords": [
    "fhevm",
    "fhe",
    "encryption",
    "ethereum"
  ],
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.9.0",
    "tfhe": "^0.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
